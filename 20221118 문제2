class Solution {
    public String solution(int num) {
        // 문제 2
        // 정수 num이 짝수일 경우 "Even"을 반환하고 홀수인 경우 "Odd"를 반환하는 함수, solution을 완성해주세요.
    
        String answer = "";
        if (num % 2 == 0) {
            answer = "Even";  // num 을 2로 나눈 결과가 0 이면 짝수니까 answer 의 문자 리럴을 Even 으로
        } else {
            answer = "Odd";   // 위에 것이 맞지 않으면 1 이기 때문에 홀수니까 answer 의 문자 리터럴을 Odd 로 한다.
        }
        return answer;
    }
}
